<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="541.0" prefWidth="468.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.dep7.sms_lite.controller.MainFormController">
   <children>
      <Label layoutX="72.0" layoutY="4.0" text="Student Management System">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="txtID" disable="true" editable="false" layoutX="15.0" layoutY="38.0" prefHeight="29.0" prefWidth="335.0" promptText="ID" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtName" layoutX="15.0" layoutY="77.0" prefHeight="29.0" prefWidth="335.0" promptText="Name" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtPhone" layoutX="15.0" layoutY="116.0" onAction="#txtPhone_OnAction" promptText="Phone (xxx-xxxxxxx)" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="20.0">
         <font>
            <Font size="15.0" />
         </font>
      </TextField>
      <Button fx:id="btnSave" layoutX="358.0" layoutY="211.0" mnemonicParsing="false" onAction="#btnSave_OnAction" prefHeight="29.0" prefWidth="64.0" text="Save">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Button layoutX="240.0" layoutY="194.0" mnemonicParsing="false" onAction="#btnClear_OnAction" prefHeight="26.0" prefWidth="72.0" text="Clear" />
      <Button layoutX="240.0" layoutY="234.0" mnemonicParsing="false" onAction="#btnRemove_OnAction" text="Remove" />
      <ListView fx:id="lstContact" layoutX="14.0" layoutY="160.0" prefHeight="147.0" prefWidth="185.0" AnchorPane.leftAnchor="20.0" />
      <TableView fx:id="tblStudents" layoutX="13.0" layoutY="326.0" prefHeight="200.0" prefWidth="442.0">
        <columns>
          <TableColumn prefWidth="93.0" text="ID" />
          <TableColumn prefWidth="228.0" text="Name" />
            <TableColumn prefWidth="126.0" text="Delete" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
